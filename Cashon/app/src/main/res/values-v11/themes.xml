<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="cashguru" parent="@style/_cashguru"/>

    <style name="_cashguru" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="editTextBackground">@drawable/cashguru_edit_text_holo_light</item>

        <item name="android:textColorHighlight">#99e35555</item>
        <item name="android:textSelectHandleLeft">@drawable/cashguru_text_select_handle_left</item>
        <item name="android:textSelectHandleRight">@drawable/cashguru_text_select_handle_right</item>
        <item name="android:textSelectHandle">@drawable/cashguru_text_select_handle_middle</item>

        <item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewcashguru</item>

        <item name="android:listChoiceIndicatorMultiple">@drawable/cashguru_btn_check_holo_light</item>

        <item name="android:listChoiceIndicatorSingle">@drawable/cashguru_btn_radio_holo_light</item>

        <item name="buttonStyle">@style/Buttoncashguru</item>

        <item name="imageButtonStyle">@style/ImageButtoncashguru</item>

        <item name="android:dropDownSpinnerStyle">@style/Spinnercashguru</item>

        <item name="android:progressBarStyleHorizontal">@style/ProgressBarcashguru</item>

        <item name="seekBarStyle">@style/SeekBarcashguru</item>

        <item name="ratingBarStyle">@style/RatingBarcashguru</item>

        <item name="android:ratingBarStyleIndicator">@style/RatingBarBigcashguru</item>

        <item name="android:ratingBarStyleSmall">@style/RatingBarSmallcashguru</item>

        <item name="android:buttonStyleToggle">@style/Togglecashguru</item>

        <item name="listChoiceBackgroundIndicator">@drawable/cashguru_list_selector_holo_light</item>

        <item name="android:activatedBackgroundIndicator">@drawable/cashguru_activated_background_holo_light</item>

        <item name="android:fastScrollThumbDrawable">@drawable/cashguru_fastscroll_thumb_holo</item>
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="checkboxStyle">@style/CheckBoxcashguru</item>
        <item name="radioButtonStyle">@style/RadioButtoncashguru</item>
        <item name="autoCompleteTextViewStyle">@style/AutoCompleteTextViewcashguru</item>

        <item name="spinnerStyle">@style/Spinnercashguru</item>

        <item name="spinnerDropDownItemStyle">@style/SpinnerDropDownItemcashguru</item>

    </style>
</resources>