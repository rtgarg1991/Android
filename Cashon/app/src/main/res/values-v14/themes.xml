<?xml version="1.0" encoding="utf-8"?>
<resources>
        <style name="cashguru" parent="@style/_cashguru"/>
        <style name="_cashguru" parent="android:Theme.Holo.Light.DarkActionBar">

                <!-- API 14 theme customizations can go here. -->
                <item name="android:dropDownSpinnerStyle">@style/Spinnercashguru</item>
                <item name="android:listChoiceIndicatorSingle">@drawable/cashguru_btn_radio_holo_light</item>
                <item name="android:editTextBackground">@drawable/cashguru_edit_text_holo_light</item>
                <item name="android:listChoiceIndicatorMultiple">@drawable/cashguru_btn_check_holo_light</item>
                <!--<item name="android:progressBarStyleLarge">@style/ProgressBarStyle</item>-->
                <item name="android:dropDownListViewStyle">@style/ListViewAppTheme</item>
