<resources>

    <style name="EditTextcashguru" parent="android:Widget.EditText">
        <item name="android:background">@drawable/cashguru_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="AutoCompleteTextViewcashguru" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/cashguru_list_selector_holo_light</item>
        <item name="android:background">@drawable/cashguru_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="CheckBoxcashguru" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/cashguru_btn_check_holo_light</item>
    </style>

    <style name="RadioButtoncashguru" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/cashguru_btn_radio_holo_light</item>
    </style>

    <style name="Buttoncashguru" parent="android:Widget.Button">
        <item name="android:background">@drawable/cashguru_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtoncashguru" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/cashguru_btn_default_holo_light</item>
    </style>

    <style name="Spinnercashguru" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/cashguru_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/cashguru_list_selector_holo_light</item>
    </style>

    <style name="SpinnerDropDownItemcashguru" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/cashguru_btn_radio_holo_light</item>
    </style>
    <style name="Tabcashguru">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/cashguru_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextcashguru">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarcashguru" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/cashguru_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/cashguru_progress_indeterminate_horizontal_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="SeekBarcashguru" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/cashguru_scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/cashguru_scrubber_progress_horizontal_holo_light</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/cashguru_scrubber_control_selector_holo_light</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>

    <style name="RatingBarcashguru" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/cashguru_ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/cashguru_ratingbar_full_holo_light</item>
    </style>

    <style name="RatingBarBigcashguru" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/cashguru_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/cashguru_ratingbar_holo_light</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:maxHeight">35dip</item>
    </style>

    <style name="RatingBarSmallcashguru" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/cashguru_ratingbar_small_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/cashguru_ratingbar_small_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="Togglecashguru" parent="android:Widget.Button.Toggle">
        <item name="android:background">@drawable/cashguru_btn_toggle_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ListViewcashguru" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/cashguru_list_selector_holo_light</item>
    </style>

    <style name="ListViewcashguru.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/cashguru_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemcashguru" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

    <!-- EditText Style-->
    <style name="textview_style">
        <item name="android:layout_marginLeft">@dimen/textview_style_margin_left</item>
        <item name="android:layout_marginRight">@dimen/textview_style_margin_right</item>
        <item name="android:layout_marginTop">@dimen/textview_style_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/textview_style_margin_bottom</item>

        <item name="android:paddingLeft">@dimen/textview_style_padding_left</item>
        <item name="android:paddingRight">@dimen/textview_style_padding_right</item>
        <item name="android:paddingTop">@dimen/textview_style_padding_top</item>
        <item name="android:paddingBottom">@dimen/textview_style_padding_bottom</item>
        <item name="android:textColor">@color/primary_text_color</item>
    </style>
    <style name="edittext_style">
        <item name="android:layout_marginLeft">@dimen/edittext_style_margin_left</item>
        <item name="android:layout_marginRight">@dimen/edittext_style_margin_right</item>
        <item name="android:layout_marginTop">@dimen/edittext_style_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/edittext_style_margin_bottom</item>

        <item name="android:paddingLeft">@dimen/edittext_style_padding_left</item>
        <item name="android:paddingRight">@dimen/edittext_style_padding_right</item>
        <item name="android:paddingTop">@dimen/edittext_style_padding_top</item>
        <item name="android:paddingBottom">@dimen/edittext_style_padding_bottom</item>
        <item name="android:textColor">@color/primary_text_color</item>
        <item name="android:textColorHint">@color/primary_text_color</item>
    </style>
    <style name="edittext_single_line_style" parent="edittext_style">
        <item name="android:gravity">center</item>
        <item name="android:layout_height">@dimen/edittext_style_layout_height</item>
        <!--<item name="android:background">@drawable/edittext_style</item>-->
        <item name="android:singleLine">true</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="app_install_title_textview_style" parent="edittext_single_line_style">
        <item name="android:paddingTop">@dimen/app_install_textview_style_padding_top</item>
        <item name="android:paddingBottom">@dimen/app_install_textview_style_padding_bottom</item>
        <item name="android:layout_marginTop">@dimen/app_install_textview_style_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/app_install_textview_style_margin_bottom</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="app_install_description_textview_style" parent="app_install_title_textview_style">
        <item name="android:layout_marginLeft">@dimen/app_install_description_style_padding_left</item>
        <item name="android:layout_marginRight">@dimen/app_install_description_style_padding_right</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:gravity">left</item>
    </style>
    <style name="edittext_single_line_style_no_margin" parent="edittext_single_line_style">
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="drawer_list_textview_style" parent="edittext_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/drawer_list_item_padding_left</item>
        <item name="android:paddingRight">@dimen/drawer_list_item_padding_right</item>
        <item name="android:textSize">@dimen/drawer_list_item_text_size</item>
        <item name="android:minHeight">@dimen/drawer_list_item_min_height</item>
        <item name="android:textColor">@color/drawer_list_item_text_color</item>
        <item name="android:layout_marginTop">@dimen/drawer_list_item_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/drawer_list_item_margin_bottom</item>
    </style>

    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/cashguru_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/cashguru_list_selector_holo_light</item>
    </style>

    <!-- Changes the spinner drop down item radio button style -->
    <style name="DropDownItemSpinnerTheme" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/cashguru_btn_radio_holo_light</item>
    </style>

    <style name="ListViewSpinnerTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/cashguru_list_selector_holo_light</item>
    </style>

    <style name="ListViewSpinnerTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/cashguru_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemTheme"
        parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style><!--

    <style name="ProgressBarStyle"
        parent="android:Widget.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@drawable/progressbar</item>
    </style>-->
    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>
</resources>
